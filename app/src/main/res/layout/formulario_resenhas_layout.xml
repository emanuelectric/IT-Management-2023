<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="43dp"
        android:layout_height="35dp"
        app:srcCompat="@drawable/arrows"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="10dp"
        android:clickable="true"
        android:onClick="LanzarVistaMenuPrincipal" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="162dp"
        android:layout_height="35dp"
        android:text="Reseñas"
        android:textSize="25dp"
        android:layout_marginLeft="60dp"
        android:layout_marginTop="30dp"
        />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="247dp"
        android:layout_height="101dp"
        android:layout_marginLeft="80dp"
        android:layout_marginTop="80dp"
        app:srcCompat="@drawable/logo" />


    <TextView
        android:id="@+id/textView1"
        android:layout_width="400dp"
        android:layout_height="100dp"
        android:layout_marginLeft="60dp"
        android:layout_marginTop="200dp"
        android:text="¿Que te parecio el servicio?"
        android:textSize="25dp" />

    <Button
        android:id="@+id/imageView16"
        android:layout_width="250dp"
        android:layout_height="50dp"
        android:layout_marginTop="270dp"
        android:layout_marginLeft="80dp"
        android:textColor="@color/white"
        android:background="@color/dark_grey"
        android:text="Satisfactorio" />

    <Button
        android:id="@+id/imageView17"
        android:layout_width="250dp"
        android:layout_height="50dp"
        android:layout_marginLeft="80dp"
        android:layout_marginTop="350dp"
        android:textColor="@color/white"
        android:text="Aceptable" />

    <Button
        android:id="@+id/imageView18"
        android:layout_width="250dp"
        android:layout_height="50dp"
        android:layout_marginTop="430dp"
        android:layout_marginLeft="80dp"
        android:textColor="@color/white"
        android:text="Bueno" />

    <Button
        android:id="@+id/imageView19"
        android:layout_width="250dp"
        android:layout_height="50dp"
        android:layout_marginLeft="80dp"
        android:layout_marginTop="510dp"
        android:textColor="@color/white"

        android:text="Pesimo" />

    <Button
        android:id="@+id/boton_continuar_registrar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/imageView19"
        android:layout_marginTop="20dp"
        android:onClick="enviarFormularioResenia"
        android:text="Enviar"
        android:textSize="16sp"
        android:background="@drawable/boton_redondeado_rojo"
        android:textColor="@color/white"
        android:padding="12dp" />


</RelativeLayout>